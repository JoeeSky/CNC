<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class dynamic-insert="true" dynamic-update="true" name="ManufacturetaskDetails">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="integer">
   <generator class="native"/>
  </id>
  
  <property generated="never" lazy="false" length="12" name="sn"
   not-null="true" type="string"/>
  <many-to-one cascade="save-update" class="Manufacturetask" column="task_id"
   lazy="proxy" name="task" not-null="true"/>
  <property generated="never" lazy="false" name="description"
   not-null="true" type="text"/>
  <property generated="never" lazy="false" length="10" name="type"
   not-null="true" type="integer"/>
  <property generated="never" lazy="false" length="20" name="file_name"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="200" name="file_path"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="10" name="isdown"
   not-null="true" type="boolean"/>
 </class>
</hibernate-mapping>