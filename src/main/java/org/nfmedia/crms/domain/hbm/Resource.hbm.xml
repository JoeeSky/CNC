<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class dynamic-insert="true" dynamic-update="true" name="Resource">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="short">
   <meta attribute="scope-set">private</meta>
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" length="10" name="name"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="50" name="url"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="10" name="parentName"
   not-null="true" type="string"/>
 </class>
</hibernate-mapping>
