<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class name="Role">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="integer">
   <generator class="native"/>
  </id>
  <property length="40" name="name" type="string"/>
  <property length="20" name="companyType" type="string"/>
  <property length="200" name="description" type="string"/>
  <property name="enable" type="boolean"/>
  <property length="6" name="sortOrder" type="integer"/>
</class>
</hibernate-mapping>
