<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class dynamic-insert="true" dynamic-update="true" name="Programtask">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="integer">
   <generator class="native"/>
  </id>
  
  <property generated="never" lazy="false" length="12" name="sn"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="200" name="task_name"
   not-null="true" type="string"/>
  <many-to-one cascade="save-update" class="Demander" column="demander_id"
   lazy="proxy" name="demander" not-null="true"/>
  <many-to-one cascade="save-update" class="User" column="demander_user_id"
   lazy="proxy" name="demander_user" not-null="true"/>
  <property generated="never" lazy="false" name="issue_time"
   not-null="true" type="date"/>
  <property generated="never" lazy="false" name="modify_time"
   not-null="true" type="date"/>
  <property generated="never" lazy="false" name="description"
   not-null="true" type="text"/>
  <many-to-one cascade="save-update" class="Manufacturer" column="manufacturer_id"
   lazy="proxy" name="manufacturer" not-null="true"/>
  <property generated="never" lazy="false" length="10" name="status"
   not-null="true" type="integer"/>
   
 </class>
</hibernate-mapping>