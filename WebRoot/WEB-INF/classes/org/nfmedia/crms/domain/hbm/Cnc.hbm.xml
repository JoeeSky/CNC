<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class dynamic-insert="true" dynamic-update="true" name="Cnc">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="short">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" length="50" name="name"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" length="500"
   name="description" not-null="true" type="string"/>
  <property lazy="false" length="100" name="pinyin" not-null="true" type="string"/>
  <property lazy="false" length="200" name="address" not-null="true" type="string"/>
  <property lazy="false" length="100" name="contact" not-null="true" type="string"/>
  <property lazy="false" length="100" name="email" not-null="true" type="string"/>
  <property length="100" name="mobile" type="string"/>
 </class>
</hibernate-mapping>
