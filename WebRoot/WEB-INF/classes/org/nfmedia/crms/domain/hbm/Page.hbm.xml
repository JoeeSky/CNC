<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.nfmedia.crms.domain">
 <class name="Page">
  <meta attribute="extends">BaseDomain</meta>
  <id name="id" type="integer">
   <generator class="native"/>
  </id>
  <property length="50" name="name" type="string"/>
  <property length="100" name="url" type="string"/>
  <property length="20" name="parentName" type="string"/>
  <property length="10" name="sortOrder" type="integer"/>
  <property length="100" name="companyTypes" type="string"/>
 </class>
</hibernate-mapping>
